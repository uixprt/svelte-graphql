{
	"extends": "./.svelte-kit/tsconfig.json",
	"lib": ["ES2015"],
	"compilerOptions": {
		"baseUrl": "src",
		"paths": {
			"styles": ["styles/*"]
		},
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	}
}
